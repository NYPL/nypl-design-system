{
  "name": "@nypl/design-system-react-components",
  "version": "0.25.3",
  "description": "Design System React Components",
  "repository": {
    "type": "git",
    "url": "https://github.com/NYPL/nypl-design-system/"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/design-system-react-components.esm.js",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">10"
  },
  "author": "NYPL",
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build && npm run generate-sass-resources",
    "test": "tsdx test --passWithNoTests",
    "test:watch": "tsdx test - --watch",
    "test:generate-output": "tsdx test --json --outputFile=.jest-test-results.json  || true",
    "lint": "tsdx lint",
    "prepare": "npm run build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "generate-sass-resources": "gulp",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "npm run prebuild:storybook && build-storybook -c .storybook -o ./storybook-static",
    "prebuild:storybook": "npm run test:generate-output",
    "stylelint": "stylelint \"**/*.scss\""
  },
  "dependencies": {
    "@chakra-ui/react": "1.7.1",
    "@chakra-ui/system": "1.8.1",
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "@mdx-js/react": "^1.6.22",
    "cpy-cli": "^3.1.1",
    "framer-motion": "^4.1.17",
    "he": "^1.2.0",
    "react-datepicker": "4.1.1",
    "react-uid": "^2.3.0",
    "system-font-css": "^2.0.2",
    "typescript": "^4.3.5",
    "uuid": "^8.3.2"
  },
  "peerDependencies": {
    "react": ">=16.13.0",
    "react-dom": ">=16.13.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": false,
    "trailingComma": "es5"
  },
  "size-limit": [
    {
      "path": "dist/design-system-react-components.cjs.production.min.js",
      "limit": "300 KB"
    },
    {
      "path": "dist/design-system-react-components.esm.js",
      "limit": "300 KB"
    }
  ],
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@size-limit/preset-small-lib": "^5.0.1",
    "@storybook/addon-a11y": "^6.2.9",
    "@storybook/addon-actions": "^6.2.9",
    "@storybook/addon-docs": "^6.2.9",
    "@storybook/addon-essentials": "^6.3.2",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-jest": "^6.3.6",
    "@storybook/addon-links": "^6.2.9",
    "@storybook/addon-queryparams": "6.2.9",
    "@storybook/addons": "^6.3.2",
    "@storybook/react": "^6.3.2",
    "@svgr/rollup": "^5.5.0",
    "@svgr/webpack": "^5.5.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "26.0.20",
    "@types/jest-axe": "3.5.1",
    "@types/jsdom": "^12.2.4",
    "@types/node": "^12.7.12",
    "@types/react": "^16.9.5",
    "@types/testing-library__jest-dom": "^5.14.1",
    "@typescript-eslint/eslint-plugin": "^4.12.0",
    "@typescript-eslint/parser": "^4.12.0",
    "autoprefixer": "^9.8.6",
    "babel-loader": "^8.2.2",
    "breakpoint-sass": "^2.7.1",
    "css-loader": "^5.2.7",
    "cssnano": "^5.0.7",
    "eslint": "^7.31.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-flowtype": "^5.8.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "gulp": "^4.0.2",
    "gulp-scss-combine": "^1.0.0",
    "husky": "^4.2.5",
    "import-glob-loader": "^1.1.0",
    "jest": "^27.0.6",
    "jest-axe": "4.1.0",
    "jsdom": "^15.2.1",
    "node-sass": "^4.14.1",
    "node-sass-glob-importer": "^5.3.2",
    "normalize.css": "^8.0.1",
    "postcss": "8.3.6",
    "prettier": "2.1.1",
    "pretty-quick": "^3.1.1",
    "react": "16.14.0",
    "react-autosuggest": "^10.0.2",
    "react-docgen-typescript-loader": "^3.1.0",
    "react-dom": "16.14.0",
    "react-is": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "react-test-renderer": "^16.14.0",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-svg": "^2.0.0",
    "sass": "^1.35.1",
    "sass-loader": "^8.0.2",
    "semantic-release-slack-bot": "^2.1.0",
    "size-limit": "^5.0.1",
    "storybook-addon-designs": "^5.4.2",
    "style-loader": "^2.0.0",
    "stylelint": "^13.13.1",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-order": "^4.1.0",
    "stylelint-prettier": "^1.1.2",
    "stylelint-scss": "^3.18.0",
    "ts-jest": "^27.0.4",
    "ts-loader": "^6.2.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.0"
  }
}
