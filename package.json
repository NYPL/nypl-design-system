{
    "name": "@nypl/design-system-react-components",
    "version": "0.7.2",
    "description": "Design System React Components",
    "repository": {
        "type": "git",
        "url": "https://github.com/NYPL/nypl-design-system/"
    },
    "main": "dist/design-system-react-components.min.js",
    "types": "lib/index.d.ts",
    "author": "NYPL",
    "license": "Apache-2.0",
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "start": "node node_modules/@storybook/react/bin/index -p 9001 -c .storybook-react",
        "build-storybook": "node node_modules/.bin/build-storybook -c .storybook-react -o ./storybook-static",
        "test": "npm run lint && npm run test:ci",
        "test:ci": "npm run build-ts && cp src/styles.scss lib && mocha --require lib/__tests__/setupEnzyme.js --recursive lib/**/*.test.js",
        "format": "prettier --write 'src/**/*.{ts, tsx, js,jsx,json}'",
        "pretty-quick": "pretty-quick",
        "prepare": "npm run dist",
        "build-ts": "rm -rf lib && tsc",
        "dist": "rm -rf dist && webpack --mode production --progress --display-modules --config webpack.config.js",
        "lint": "tslint -c tslint.json src/__tests__/components/*.tsx src/components/**/**/*.tsx src/components/**/**/**/*.tsx src/utils/*.tsx",
        "watch-webpack": "webpack --mode development --watch --progress --display-modules --config webpack.config.js"
    },
    "dependencies": {
        "he": "^1.2.0",
        "react": "^16.13.1",
        "react-router-dom": "^5.2.0",
        "react-uid": "^2.3.0",
        "system-font-css": "^2.0.2",
        "typescript": "^3.6.4"
    },
    "husky": {
        "hooks": {
            "pre-commit": "pretty-quick --staged"
        }
    },
    "devDependencies": {
        "@babel/core": "^7.6.4",
        "@babel/plugin-proposal-class-properties": "^7.5.5",
        "@babel/plugin-proposal-export-default-from": "^7.5.2",
        "@babel/plugin-transform-arrow-functions": "^7.2.0",
        "@babel/preset-env": "^7.6.3",
        "@babel/preset-react": "^7.6.3",
        "@babel/preset-typescript": "^7.10.1",
        "@storybook/addon-a11y": "^5.2.4",
        "@storybook/addon-actions": "^5.2.3",
        "@storybook/addon-info": "^5.0.11",
        "@storybook/addon-knobs": "^5.2.3",
        "@storybook/addon-links": "^5.2.3",
        "@storybook/addons": "^5.2.3",
        "@storybook/react": "^5.2.3",
        "@types/chai": "^4.2.3",
        "@types/enzyme": "^3.10.3",
        "@types/jsdom": "^12.2.4",
        "@types/mocha": "^5.2.6",
        "@types/node": "^12.7.12",
        "@types/react": "^16.9.5",
        "@types/sinon": "^7.5.0",
        "@types/storybook__react": "^4.0.2",
        "awesome-typescript-loader": "^5.2.1",
        "babel-loader": "^8.0.6",
        "babel-plugin-symlink-import": "^0.1.1",
        "babel-plugin-transform-class-properties": "^6.24.1",
        "breakpoint-sass": "^2.7.1",
        "chai": "^4.2.0",
        "enzyme": "^3.10.0",
        "enzyme-adapter-react-16": "^1.15.1",
        "extract-text-webpack-plugin": "^3.0.2",
        "husky": "^4.2.5",
        "import-glob-loader": "^1.1.0",
        "jsdom": "^15.2.1",
        "mini-css-extract-plugin": "^0.9.0",
        "mocha": "^6.2.2",
        "node-sass": "^4.14.1",
        "node-sass-glob-importer": "^5.3.2",
        "normalize.css": "^8.0.1",
        "prettier": "2.0.5",
        "pretty-quick": "^2.0.1",
        "prop-types": "^15.7.2",
        "react-docgen-typescript-loader": "^3.1.0",
        "rimraf": "^3.0.0",
        "sass-loader": "^8.0.2",
        "sinon": "^7.3.0",
        "storybook-addon-designs": "^5.2.1",
        "stylelint": "^13.6.1",
        "stylelint-config-recommended": "^3.0.0",
        "stylelint-order": "^4.1.0",
        "stylelint-prettier": "^1.1.2",
        "stylelint-scss": "^3.18.0",
        "svg-inline-loader": "^0.8.0",
        "svg-sprite": "^1.5.0",
        "ts-loader": "^6.2.0",
        "tslint": "^5.0.0",
        "tslint-react-a11y": "^1.0.0",
        "twig": "^1.13.3",
        "twig-loader": "^0.5.1",
        "twigjs-loader": "^1.0.0",
        "webpack": "^4.41.5",
        "webpack-cli": "^3.3.10",
        "webpack-node-externals": "^1.7.2"
    }
}
