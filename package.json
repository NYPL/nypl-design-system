{
  "name": "@nypl/design-system-react-components",
  "version": "1.5.1",
  "description": "NYPL Reservoir Design System React Components",
  "repository": {
    "type": "git",
    "url": "https://github.com/NYPL/nypl-design-system/"
  },
  "main": "dist/design-system-react-components.umd.cjs",
  "types": "dist/src/index.d.ts",
  "module": "dist/design-system-react-components.js",
  "type": "module",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/design-system-react-components.js",
      "require": "./dist/design-system-react-components.umd.cjs"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "engines": {
    "node": ">=14 <=16"
  },
  "author": "NYPL",
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc --noEmit && vite build && npm run generate-sass-resources",
    "test": "jest --config=jest.config.js",
    "test:ci": "jest --ci --config=jest.config.js --maxWorkers=2",
    "prod": "vite build --mode=production",
    "lint": "eslint src --ext=ts,tsx --cache",
    "test:generate-output": "jest --config=jest.config.js --json --outputFile=.jest-test-results.json || true",
    "prepare": "npm run build && husky install",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "generate-sass-resources": "gulp",
    "storybook": "start-storybook -p 6006 -s ./.storybook/public",
    "build-storybook:v1": "build-storybook -c .storybook -o ./reservoir/v1",
    "prebuild:storybook": "npm run test:generate-output"
  },
  "lint-staged": {
    "*.{jsx,ts,tsx}": "eslint src --ext=jsx,ts,tsx --cache --fix",
    "*.{js,jsx,ts,tsx,css,md}": "prettier --write"
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": false,
    "trailingComma": "es5"
  },
  "size-limit": [
    {
      "path": "dist/design-system-react-components.umd.cjs",
      "limit": "300 KB"
    },
    {
      "path": "dist/design-system-react-components.js",
      "limit": "300 KB"
    }
  ],
  "dependencies": {
    "@chakra-ui/focus-lock": ">=1.2.6 <2.0.0",
    "@chakra-ui/react": ">=1.8.5 <=1.8.8",
    "@chakra-ui/system": ">=1.11.0 <=1.12.1",
    "@charlietango/use-native-lazy-loading": "1.10.0",
    "@emotion/react": ">=11.4.1",
    "@emotion/styled": ">=11.3.0",
    "downshift": "6.1.7",
    "framer-motion": "4.1.17",
    "js-cookie": "3.0.1",
    "react-datepicker": "4.1.1",
    "react-ga": "3.3.1",
    "react-intersection-observer": "9.2.2",
    "system-font-css": "2.0.2",
    "tough-cookie": "4.0.0",
    "typescript": "4.9.5",
    "vite-plugin-svgr": "^2.4.0"
  },
  "peerDependencies": {
    "@chakra-ui/focus-lock": ">=1.2.6 <2.0.0",
    "@chakra-ui/react": ">=1.8.5 <=1.8.8",
    "@chakra-ui/system": ">=1.11.0 <=1.12.1",
    "@emotion/react": ">=11.4.1",
    "@emotion/styled": ">=11.3.0",
    "framer-motion": "4.1.17",
    "react": ">=16.13.0",
    "react-dom": ">=16.13.0",
    "typescript": "4.9.5"
  },
  "devDependencies": {
    "@babel/core": "7.14.6",
    "@chakra-ui/storybook-addon": "1.0.3",
    "@jest/core": "27.3.1",
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-typescript": "^11.0.0",
    "@size-limit/preset-small-lib": "5.0.1",
    "@storybook/addon-a11y": "6.5.13",
    "@storybook/addon-actions": "6.5.13",
    "@storybook/addon-docs": "6.5.13",
    "@storybook/addon-essentials": "6.5.13",
    "@storybook/addon-jest": "6.5.13",
    "@storybook/addon-links": "6.5.13",
    "@storybook/addons": "6.5.13",
    "@storybook/react": "6.5.13",
    "@svgr/webpack": "5.5.0",
    "@testing-library/jest-dom": "5.14.1",
    "@testing-library/react": "12.0.0",
    "@testing-library/react-hooks": "7.0.2",
    "@testing-library/user-event": "13.2.1",
    "@types/jest": "29.4.0",
    "@types/jest-axe": "3.5.3",
    "@types/node": "18.13.0",
    "@types/react": "16.9.41",
    "@types/testing-library__jest-dom": "5.14.1",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "@vitejs/plugin-react": "^3.1.0",
    "breakpoint-sass": "2.7.1",
    "css-loader": "5.2.7",
    "eslint": "7.31.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-storybook": "0.5.12",
    "gulp": "4.0.2",
    "gulp-concat": "2.6.1",
    "husky": "7.0.4",
    "jest": "29.4.3",
    "jest-axe": "5.0.1",
    "jest-environment-jsdom": "^29.5.0",
    "lint-staged": "10.5.4",
    "node-sass-glob-importer": "5.3.2",
    "normalize.css": "8.0.1",
    "prettier": "2.4.1",
    "react": "17.0.2",
    "react-autosuggest": "10.0.2",
    "react-docgen-typescript-loader": "3.7.2",
    "react-dom": "17.0.2",
    "react-test-renderer": "17.0.2",
    "sass": "^1.60.0",
    "sass-loader": "10.0.0",
    "size-limit": "5.0.1",
    "storybook-addon-designs": "6.2.1",
    "style-loader": "2.0.0",
    "ts-jest": "29.0.5",
    "tslib": "2.3.0",
    "vite": "^4.2.1",
    "vite-plugin-dts": "^2.1.0"
  }
}
