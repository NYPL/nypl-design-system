{
  "name": "@nypl/design-system-react-components",
  "version": "0.23.3",
  "description": "Design System React Components",
  "repository": {
    "type": "git",
    "url": "https://github.com/NYPL/nypl-design-system/"
  },
  "main": "dist/design-system-react-components.min.js",
  "types": "lib/index.d.ts",
  "author": "NYPL",
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "start": "node node_modules/@storybook/react/bin/index -p 9001 -c .storybook",
    "build-storybook": "node node_modules/.bin/build-storybook -c .storybook -o ./storybook-static",
    "test": "npm run lint && npm run test:ci",
    "test:ci": "npm run build-ts && cpy src/styles.scss lib && mocha --require lib/__tests__/setupEnzyme.js --recursive lib/**/*.test.js",
    "format": "prettier --write 'src/**/*.{ts, tsx, js,jsx,json}'",
    "pretty-quick": "pretty-quick",
    "prepare": "npm run dist",
    "build-ts": "rimraf lib && tsc",
    "generate-sass-resources": "gulp",
    "dist": "rimraf dist && webpack --mode production --progress --display-modules --config webpack.config.js && npm run generate-sass-resources",
    "stylelint": "stylelint \"**/*.scss\"",
    "lintts:fix": "eslint --fix --ext=jsx,ts,tsx src",
    "lint": "eslint --ext=jsx,ts,tsx src && npm run stylelint",
    "watch-webpack": "webpack --mode development --watch --progress --display-modules --config webpack.config.js"
  },
  "dependencies": {
    "cpy-cli": "^3.1.1",
    "he": "^1.2.0",
    "react-router-dom": "^5.2.0",
    "react-uid": "^2.3.0",
    "system-font-css": "^2.0.2",
    "typescript": "^3.9.2",
    "uuid": "^8.3.2"
  },
  "peerDependencies": {
    "react": ">=16.13.0",
    "react-dom": ">=16.13.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-export-default-from": "^7.5.2",
    "@babel/plugin-transform-arrow-functions": "^7.2.0",
    "@storybook/addon-a11y": "^6.2.9",
    "@storybook/addon-actions": "^6.2.9",
    "@storybook/addon-docs": "^6.2.9",
    "@storybook/addon-essentials": "^6.2.9",
    "@storybook/addon-knobs": "^6.2.9",
    "@storybook/addon-links": "^6.2.9",
    "@storybook/addon-queryparams": "^6.2.9",
    "@storybook/react": "^6.2.9",
    "@svgr/webpack": "^5.5.0",
    "@types/chai": "^4.2.3",
    "@types/enzyme": "^3.10.3",
    "@types/jsdom": "^12.2.4",
    "@types/mocha": "^5.2.6",
    "@types/node": "^12.7.12",
    "@types/react": "^16.9.5",
    "@types/sinon": "^7.5.0",
    "@typescript-eslint/eslint-plugin": "^4.12.0",
    "@typescript-eslint/parser": "^4.12.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-symlink-import": "^0.1.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "breakpoint-sass": "^2.7.1",
    "chai": "^4.2.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "eslint": "^7.17.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "gulp": "^4.0.2",
    "gulp-scss-combine": "^1.0.0",
    "husky": "^4.2.5",
    "import-glob-loader": "^1.1.0",
    "jsdom": "^15.2.1",
    "lodash": "^4.17.20",
    "mini-css-extract-plugin": "^0.9.0",
    "mocha": "^8.3.2",
    "node-sass": "^4.14.1",
    "node-sass-glob-importer": "^5.3.2",
    "normalize.css": "^8.0.1",
    "prettier": "2.1.1",
    "pretty-quick": "^2.0.1",
    "prop-types": "^15.7.2",
    "react": "16.14.0",
    "react-autosuggest": "^10.0.2",
    "react-docgen-typescript-loader": "^3.1.0",
    "react-dom": "16.14.0",
    "react-is": "^16.13.1",
    "rimraf": "^3.0.2",
    "sass-loader": "^8.0.2",
    "semantic-release-slack-bot": "^2.1.0",
    "sinon": "^7.3.0",
    "storybook-addon-designs": "^5.4.2",
    "stylelint": "^13.13.1",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-order": "^4.1.0",
    "stylelint-prettier": "^1.1.2",
    "stylelint-scss": "^3.18.0",
    "svg-inline-loader": "^0.8.2",
    "svg-sprite": "^1.5.0",
    "ts-loader": "^6.2.0",
    "twig": "^1.13.3",
    "twig-loader": "^0.5.1",
    "twigjs-loader": "^1.0.0",
    "webpack": "^4.46.0",
    "webpack-cli": "^3.3.12"
  }
}
